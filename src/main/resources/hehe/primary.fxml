<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="385.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hehe.PrimaryController">
   <children>
      <TableView fx:id="dbTable" prefHeight="263.0" prefWidth="700.0" stylesheets="@styles.css">
        <columns>
          <TableColumn fx:id="columnId" prefWidth="20.0" text="ID" />
          <TableColumn fx:id="columnOznaczenie" prefWidth="70.0" text="Oznaczenie" />
            <TableColumn fx:id="columnNazwa" prefWidth="150.0" text="Nazwa" />
            <TableColumn fx:id="columnOpis" prefWidth="300.0" text="Opis" />
            <TableColumn fx:id="columnRodzaj" minWidth="0.0" prefWidth="100.0" text="Rodzaj" />
            <TableColumn fx:id="columnPionowy" prefWidth="30.0" text="P" />
            <TableColumn fx:id="columnJednaStrona" prefWidth="30.0" text="JS" />
        </columns>
      </TableView>
      <Label text="Primary View" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
